
// Negative exponential with thick initial condition

class Main(simulator)
  private 
    h    := [-1.0..1.0] ; h' := 0;
    mode := ""
  end
  switch mode
    case ""
      h' = -h;
end;
  simulator.endTime := 2;
  simulator.minSolverStep := 0.1;
  simulator.minComputationImprovement := 0.1;
  simulator.minTimeStep := 0.1;
  simulator.maxTimeStep := 0.2;
end

